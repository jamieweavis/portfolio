{
  "version": 2,
  "name": "website",
  "regions": ["bru"],
  "alias": ["www.jamieweavis.dev", "jamieweavis.dev"],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "status": 301,
      "headers": { "Location": "https://jamieweavis.dev/$1" }
    }
  ]
}
