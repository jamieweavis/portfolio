{
  "name": "website",
  "version": "1.0.0",
  "description": "My website created with Nuxt & Now",
  "author": "Jamie Weavis <jamie.weavis@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue .",
    "fmt": "prettier --write '**/*.{vue,js}'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "nuxt": "^2.3.1",
    "qonfucius-nuxt-fontawesome": "^0.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.0.0",
    "husky": "^1.1.4",
    "lint-staged": "^8.0.5",
    "node-sass": "^4.10.0",
    "prettier": "^1.15.2",
    "sass-loader": "^7.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.vue": [
      "prettier --write",
      "eslint --ext .vue",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  }
}
